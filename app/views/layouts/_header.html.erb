                      <% end %>
                      <div class="dropdown-divider"></div>
                      <!-- <a class="dropdown-item" href="/dairies/sign_in"><i class="dropdown-icon mdi mdi-compass-outline"></i> Log In</a> -->
                      <% if current_dairy.present? %>
                        <%= link_to("<i class='dropdown-icon mdi mdi-logout-variant'></i> #{t(:logout)}".html_safe, destroy_dairy_session_path, method: :delete, class: "dropdown-item") %> 
                      <% else %>
                        <a class="dropdown-item" href="/dairies/sign_in"><i class="dropdown-icon mdi mdi-compass-outline"></i> Log In</a>
                      <% end %>
                    </div>
