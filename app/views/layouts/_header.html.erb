                        <a class="dropdown-item" href="#"><i class="dropdown-icon mdi mdi-account-outline"></i> <%= t(:profile) %></a>
                        <a class="dropdown-item" href="#"><i class="dropdown-icon mdi mdi-settings"></i> <%= t(:setting) %></a>
